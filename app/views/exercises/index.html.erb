<h1>Exercises</h1>
<% @exercises.each do |exercise| %>
  <ul class="list-group">
    <li class="list-group-item"><h5>Name:</h5> <%= exercise.name %></li>
    <li class="list-group-item"><h5>Description:</h5> <%= exercise.description %></li>
    <li class="list-group-item"><h5>Muscle Group:</h5> <%= exercise.muscle_group %></li>
  </ul>
  <% end %>
